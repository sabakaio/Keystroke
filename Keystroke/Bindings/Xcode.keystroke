name: XCode

operations:
  - name: Show Debug Area
    hotkey: cmd-shift-y
  - name: Find in Workspace
    hotkey: cmd-shift-f
  - name: Open Quickly
    hotkey: cmd-shift-o

bindings:
  - key: v
    name: view
    bindings:
      - key: d
        name: debug
        operation: Show Debug Area
  - key: f
    name: find
    bindings:
      - key: w
        name: in Workspace
        operation: Find in Workspace
      - key: f
        name: fuzzy
        operation: Open Quickly
